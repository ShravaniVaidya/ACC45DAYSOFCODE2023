#include <iostream>
using namespace std;
int main()
 {
int x,y,ans;
cout<<"enter the number of students: ";
cin>>x;
cout<<"enter the number of chairs already in class: ";
cin>>y;
if(x>y)
{
ans=(x-y);
cout<<"number of extra chairs needed: "<<ans;
}
else{
    cout<<"0 chairs needed.";
}

	return 0;
}
