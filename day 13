#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	 int a,b,tot=0,total=0;
	 cin>>a>>b;
	 for(int i=1;; i++)
	 {  
	    if(i%2==0)
	    {
	       tot = tot+i;
	        if(tot>b)
	        {
	            cout<<"limak"<<endl;
	            break;
	        }
	    }
	    else
	    {
	    total=total+i;
	    if(total>a)
	    {
	      cout<<"bob"<<endl;
	            break; } 
	    }
	 }
	}
	return 0;
}
